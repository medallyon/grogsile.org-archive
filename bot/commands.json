{
  "restart": {
    "description": "Restarts the bot.",
    "alias": [
      "restart"
    ],
    "arguments": {},
    "userPermissions": 900,
    "example": ""
  },

  "reload": {
    "description": "Reloads the specified partition of the bot. Reloads both partitions (web and bot) if no parameter is specified.",
    "alias": [
      "reload",
      "rl"
    ],
    "arguments": {
      "partition": {
        "description": "Can be either BOT or WEB, or none.",
        "optional": true
      }
    },
    "userPermissions": 900,
    "example": "[ BOT | WEB ]"
  },

  "evaluate": {
    "description": "Evaluates a given Expression.",
    "alias": [
      "eval",
      "evaluate"
    ],
    "arguments": {
      "expression": {
        "description": "Any JavaScript code inserted here will be evaluated.",
        "optional": false
      }
    },
    "userPermissions": 900,
    "example": "\"Hello There, World!\""
  },

  "about": {
    "description": "Shows the `about` panel.",
    "alias": [
      "about",
      "info",
      "help"
    ],
    "arguments": {},
    "userPermissions": 100,
    "example": ""
  },

  "commands": {
    "description": "Displays some help with commands.",
    "alias": [
      "commands",
      "command",
      "man"
    ],
    "arguments": {
      "command": {
        "description": "The target command for which help should be displayed.",
        "optional": true
      }
    },
    "userPermissions": 100,
    "example": "ping"
  },

  "ping": {
    "description": "Pong!",
    "alias": [
      "ping"
    ],
    "arguments": {
      "text": {
        "description": "Text that will be displayed instead of the factory default 'Pong!'.",
        "optional": true
      }
    },
    "userPermissions": 100,
    "example": "[ CHIM. Those who know it can reshape the land. Witness the home of the Red King Once Jungled. ]"
  },

  "wiki": {
    "description": "Searches the UESP for user input.",
    "alias": [
      "wiki",
      "uesp"
    ],
    "arguments": {
      "input": {
        "description": "The input to search the wiki for.",
        "optional": false
      }
    },
    "userPermissions": 100,
    "example": "Norgred Hardhelm"
  },

  "esoItem": {
    "description": "Searches for a given item from Elder Scrolls Online.",
    "alias": [
      "esoItem",
      "item"
    ],
    "arguments": {
      "input": {
        "description": "The item to search for.",
        "optional": false
      }
    },
    "userPermissions": 100,
    "example": "Valkyn Skoria's Hat"
  },

  "profile": {
    "description": "Displays your ESOI profile.",
    "alias": [
      "profile"
    ],
    "arguments": {
      "user": {
        "description": "An optional @mention to show the profile for.",
        "optional": true
      }
    },
    "userPermissions": 100,
    "example": "[ @Medallyon#5012 ]"
  },

  "character": {
    "description": "Shows character summary.",
    "alias": [
      "characters",
      "chars",
      "character",
      "char"
    ],
    "arguments": {
      "user": {
        "description": "An optional @mention to show the character for.",
        "optional": true
      },
      "character": {
        "description": "The character you would like to look up. Omit this to view all characters.",
        "optional": true
      }
    },
    "userPermissions": 100,
    "example": "[ @Medallyon#5012 ] [ Medallyah ]"
  },

  "serverStatus": {
    "description": "Shows the status of the ESO Servers.",
    "alias": [
      "serverstatus",
      "servers",
      "server"
    ],
    "arguments": {
      "platform": {
        "description": "The platform to view the status of.",
        "optional": true
      }
    },
    "userPermissions": 100,
    "example": "[ PC | PS4 | XBOne ]"
  }
}
