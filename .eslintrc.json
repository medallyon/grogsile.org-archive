{
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "extends": [ "eslint:recommended" ],
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "array-bracket-spacing": [2, "never"],
    "block-scoped-var": 2,
    "brace-style": [1, "allman"],
    "camelcase": 1,
    "computed-property-spacing": [2, "never"],
    "eol-last": 2,
    "eqeqeq": [2, "smart"],
    "indent": [1, 4, { "VariableDeclarator": 0 }],
    "max-depth": 0,
    "max-statements": 0,
    "new-cap": 1,
    "no-console": 0,
    "no-extend-native": 2,
    "no-mixed-spaces-and-tabs": 2,
    "no-trailing-spaces": 2,
    "no-unused-vars": 1,
    "no-use-before-define": [2, "nofunc"],
    "object-curly-spacing": [2, "always"],
    "quotes": [2, "double", "avoid-escape"],
    "semi": [2, "always"],
    "keyword-spacing": [2, {"before": true, "after": true}],
    "space-unary-ops": 2
  },
  "globals": {
    "dClient": true,
    "__base": true,
    "__data": true,
    "__lib": true,
    "__src": true,
    "__webdir": true,
    "__botdir": true
  }
}